{
  "homepage": "https://amiguelGH.github.io/video-fsm-tres-matrix-releases-v2",
  "name": "video-fsm-tres-matrix-releases-v2",
  "version": "2.0.2",
  "private": true,
  "dependencies": {
    "@xstate/react": "4.1.1",
    "env-cmd": "^10.1.0",
    "pino": "^9.1.0",
    "pino-pretty": "^11.2.0",
    "react": "^18.1.0",
    "react-console-logger": "^0.0.2",
    "react-dom": "^18.1.0",
    "react-inject-env": "^2.1.0",
    "react-scripts": "5.0.1",
    "rxjs": "^7.8.1",
    "xstate": "^5.0.0-beta.13"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-static": "react-scripts build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.1.6",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "gh-pages": "^6.1.1",
    "semantic-release": "^24.0.0"
  },
  "release": {
    "branches": [
      "main",
      "dev",
      "qa",
      "prod"
    ],
    "repositoryUrl": "https://github.com/amiguelGH/video-fsm-tres-matrix-releases-v2.git",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "message": "chore(release): ${nextRelease.version} \n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/npm",
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "./my-build-script.sh ${nextRelease.version}"
      }
    ]
  ]
}
